{
  "projects": {
    "repox": {
      "name": "repox",
      "type": "app",
      "path": "projects/apps/repox",
      "scheme": "@app/ts",
      "build": {
        "output": "dist/repox",
        "main": "projects/apps/repox/src/main.ts",
        "assets": [
          {
            "input": "projects/apps/repox/doc/README.md",
            "output": "dist/repox"
          },
          {
            "input": "projects/apps/repox/doc/repox-logo.png",
            "output": "dist/repox"
          },
          {
            "input": "projects/apps/repox/package.json",
            "output": "dist/repox"
          }
        ]
      }
    },
    "const": {
      "name": "const",
      "type": "lib",
      "path": "projects/libs/const",
      "scheme": "@lib/ts"
    },
    "logger": {
      "name": "logger",
      "type": "lib",
      "path": "projects/libs/logger",
      "scheme": "@lib/ts"
    },
    "param-domain": {
      "name": "param-domain",
      "type": "lib",
      "path": "projects/libs/param-domain",
      "scheme": "@lib/ts"
    },
    "param-dto": {
      "name": "param-dto",
      "type": "lib",
      "path": "projects/libs/param-dto",
      "scheme": "@lib/ts"
    },
    "launcher": {
      "name": "launcher",
      "type": "lib",
      "path": "projects/libs/launcher",
      "scheme": "@lib/ts"
    },
    "repox-main": {
      "name": "repox-main",
      "type": "lib",
      "path": "projects/libs/repox/repox-main",
      "scheme": "@lib/ts"
    },
    "repox-program": {
      "name": "repox-program",
      "type": "lib",
      "path": "projects/libs/repox/repox-program",
      "scheme": "@lib/ts"
    },
    "repox-workspace": {
      "name": "repox-workspace",
      "type": "lib",
      "path": "projects/libs/repox/repox-workspace",
      "scheme": "@lib/ts"
    },
    "utils": {
      "name": "utils",
      "type": "lib",
      "path": "projects/libs/utils",
      "scheme": "@lib/ts"
    },
    "model": {
      "name": "model",
      "type": "lib",
      "path": "projects/libs/model",
      "scheme": "@lib/ts"
    },
    "repox-const": {
      "name": "repox-const",
      "type": "lib",
      "path": "projects/libs/repox/repox-const",
      "scheme": "@lib/ts"
    },
    "repox-domain": {
      "name": "repox-domain",
      "type": "lib",
      "path": "projects/libs/repox/repox-domain",
      "scheme": "@lib/ts"
    },
    "repox-website": {
      "name": "repox-website",
      "type": "app",
      "path": "projects/apps/repox-website",
      "scheme": "@blank",
      "build": {
        "output": "dist/repox-website",
        "assets": [
          {
            "input": "projects/apps/repox-website/src/*.html",
            "output": "dist/repox-website"
          },
          {
            "input": "projects/apps/repox-website/src/style.css",
            "output": "dist/repox-website"
          },
          {
            "input": "projects/apps/repox-website/src/images/**/*",
            "output": "dist/repox-website/images"
          },
          {
            "input": "projects/apps/repox-website/src/scripts/**/*",
            "output": "dist/repox-website/scripts"
          }
        ]
      }
    }
  }
}
