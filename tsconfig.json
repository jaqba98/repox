{
  "compilerOptions": {
    "target": "ES2022",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "rootDir": "./projects",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "sourceMap": true,
    "paths": {
      "@lib/const": [
        "projects/libs/const/src/index.ts"
      ],
      "@lib/logger": [
        "projects/libs/logger/src/index.ts"
      ],
      "@lib/param-domain": [
        "projects/libs/param-domain/src/index.ts"
      ],
      "@lib/param-dto": [
        "projects/libs/param-dto/src/index.ts"
      ],
      "@lib/launcher": [
        "projects/libs/repox/launcher/src/index.ts"
      ],
      "@lib/repox-main": [
        "projects/libs/repox/repox-main/src/index.ts"
      ],
      "@lib/program": [
        "projects/libs/repox/program/src/index.ts"
      ],
      "@lib/workspace": [
        "projects/libs/repox/workspace/src/index.ts"
      ],
      "@lib/utils": [
        "projects/libs/utils/src/index.ts"
      ],
      "@lib/model": [
        "projects/libs/model/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "projects/**/*.spec.ts",
    "projects/**/*.test.ts",
    "**/jest.config.ts"
  ]
}
